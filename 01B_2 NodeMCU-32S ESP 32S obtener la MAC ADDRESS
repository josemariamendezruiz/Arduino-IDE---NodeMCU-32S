#ifdef ESP32
   // SI ES UNA ESP32 UTILIZAMOS ESTA LIBRERIA 
   #include <WiFi.h>
#else
   // SI ES UNA ESP8266 UTILIZAMOS ESTA LIBRERIA 
   #include <ESP8266WiFi.h>
#endif

void setup(){
  Serial.begin(9600);
  Serial.println();
  Serial.print("MAC Address de nuestra microcontroladora: " + WiFi.macAddress());
}
 
void loop(){

}
